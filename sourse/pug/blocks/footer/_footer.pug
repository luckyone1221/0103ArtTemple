mixin footer()
	+b.FOOTER.footer
		.container
			+e.row.row.align-items-center
				.col-sm
					+e.logo(href="/")
						img(loading="lazy" src="img/svg/logo.svg" alt="")
				.col-sm-auto.d-none.d-lg-block
					+e.menu
						+mnu()
				.col-auto
					+soc()
			+e.bot-box
				+e.all-rights
					span Â© All rights reserved arTemple -
					span.set-curr-year-js 2019
